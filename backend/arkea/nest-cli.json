{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "projects": {
    "Arkea": {
      "type": "application",
      "root": "apps/Arkea",
      "entryFile": "main",
      "sourceRoot": "apps/Arkea/src",
      "compilerOptions": {
        "tsConfigPath": "apps/Arkea/tsconfig.app.json"
      }
    },
    "Characters": {
      "type": "application",
      "root": "apps/Characters",
      "entryFile": "main",
      "sourceRoot": "apps/Characters/src",
      "compilerOptions": {
        "tsConfigPath": "apps/Characters/tsconfig.app.json"
      }
    },
    "Redis": {
      "type": "library",
      "root": "libs/Redis",
      "entryFile": "index",
      "sourceRoot": "libs/Redis/src",
      "compilerOptions": {
        "tsConfigPath": "libs/Redis/tsconfig.lib.json"
      }
    }
  }
}
