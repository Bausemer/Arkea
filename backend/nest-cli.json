{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/arkea/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/arkea/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/arkea",
  "projects": {
    "arkea": {
      "type": "application",
      "root": "apps/arkea",
      "entryFile": "main",
      "sourceRoot": "apps/arkea/src",
      "compilerOptions": {
        "tsConfigPath": "apps/arkea/tsconfig.app.json"
      }
    },
    "characters": {
      "type": "application",
      "root": "apps/characters",
      "entryFile": "main",
      "sourceRoot": "apps/characters/src",
      "compilerOptions": {
        "tsConfigPath": "apps/characters/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "dtos": {
      "type": "library",
      "root": "libs/dtos",
      "entryFile": "index",
      "sourceRoot": "libs/dtos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dtos/tsconfig.lib.json"
      }
    },
    "rabbit-mq": {
      "type": "library",
      "root": "libs/rabbit-mq",
      "entryFile": "index",
      "sourceRoot": "libs/rabbit-mq/src",
      "compilerOptions": {
        "tsConfigPath": "libs/rabbit-mq/tsconfig.lib.json"
      }
    }    
  }
}